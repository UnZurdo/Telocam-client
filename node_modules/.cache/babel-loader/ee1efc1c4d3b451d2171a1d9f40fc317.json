io=\"1\"\n                  :height=\"50\"\n                  :width=\"50\"\n                  class=\"grey lighten-2\"\n                />\n              </v-flex>\n              <v-flex xs6>\n                <v-layout column wrap>\n                  <h1 class=\"heading\">{{ project.practica }}</h1>\n                  <router-link :to=\"`/empresas/${id}`\">\n                    <h3>{{ project.empresa }}</h3>\n                    <!-- <h1 v-if=\"isRepeated\">Yes</h1> -->\n                  </router-link>\n                </v-layout>\n              </v-flex>\n\n              <v-btn\n                v-if=\"!pertenceEmpresa && !checkExists\"\n                small\n                flat\n                color=\"rgb(255, 0, 0, 0.2)\"\n                slot=\"activator\"\n              >\n                <Solicitar @solicitudEnviada=\"alert = true\"/>\n              </v-btn>\n\n              <div v-if=\"pertenceEmpresa\" id=\"example\">\n                <v-btn @click=\"deleteItem\" class=\"text white--text mt-2\" color=\"grey\">\n                  <span>delete</span>\n                </v-btn>\n              </div>\n            </v-layout>\n            <v-flex xs12>\n              <v-divider></v-divider>\n            </v-flex>\n\n            <v-flex xs0 sm10 md10></v-flex>\n\n            <v-flex xs6 sm4 md3>\n              <div class=\"caption grey--text\">Studies</div>\n              <div>{{ project.studies }}</div>\n            </v-flex>\n            <v-flex class=\"mr-1\" xs10 sm10 md8>\n              <div class=\"caption grey--text\">Url</div>\n              <div>\n                <a>{{ project.urlp }}</a>\n              </div>\n            </v-flex>\n            <v-flex class=\"ml-1\" xs8 sm6 md3>\n              <div class=\"caption grey--text\">Status</div>\n              <div>{{ project.StatusP }}</div>\n            </v-flex>\n            <v-flex xs6 sm8 md3>\n              <div class=\"caption grey--text\">NÂºApplications</div>\n              <div>{{ project.applications }}</div>\n            </v-flex>\n            <v-flex xs6 sm4 md2>\n              <div class=\"caption grey--text\">Aplicaciones hasta:</div>\n              <div>{{ project.TimeDue }}</div>\n            </v-flex>\n            <v-flex xs12 sm12 md12>\n              <div class=\"caption grey--text mt-4\">Description</div>\n              <div>{{ project.Description_P }}</div>\n            </v-flex>\n          </v-layout>\n        </v-container>\n      </v-card>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Router from \"vue-router\";\n\nimport store from \"../store.js\";\nimport Solicitar from \"../components/Solicitar\";\nimport { API_BASE } from \"../config\";\n\nexport default {\n  data() {\n    return {\n      practica_empresaFK: null,\n      empresaFK: localStorage.getItem(\"empresaFK\"),\n      id: null,\n      alert: false,\n      project: \"\",\n      empresas: \"\",\n      direction: \"\",\n      token: store.getters.token,\n      DEL_URL: \"\",\n      new: \"\"\n    };\n  },\n  components: { Solicitar },\n\n  methods: {\n    sortBy(prop) {\n      if (this.direction) {\n        this.direction = false;\n        this.project.sort((a, b) => (a[prop] < b[prop] ? -1 : 1));\n      } else {\n        this.direction = true;\n        this.project.sort((a, b) => (a[prop] > b[prop] ? -1 : 1));\n      }\n    },\n    deleteItem() {\n      axios\n        .delete(`${API_BASE}/practicas/` + this.$route.params.id, {\n          data: {},\n\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: \"Bearer \" + localStorage.token\n          }\n        })\n        .then(response => {\n          this.$router.push(\"/empleos\");\n          //router.push(\"/empleos\");\n        });\n    }\n  },\n  mounted() {\n    axios\n      .get(`${API_BASE}/practicas&empresas/` + this.$route.params.id, {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: \"Bearer \" + localStorage.token\n        }\n      })\n      .then(response => {\n        console.log(\"Response\", response.data.data);\n        this.project = response.data.data;\n        this.id = response.data.data.empresa_id;\n      });\n  },\n  computed: {\n    pertenceEmpresa: function() {\n      console.log(\"empresaFK\", this.empresaFK);\n      if (this.empresaFK == this.id) return true;\n      else return false;\n    },\n    checkExists: function() {\n      const url =\n        `${API_BASE}/practicas/check/` +\n        this.$route.params.id +\n        \"/\" +\n        localStorage.getItem(\"user\");\n      console.log(url);\n      axios\n        .get(url, {\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: \"Bearer \" + localStorage.token\n          }\n        })\n        .then(response => {\n          console.log(\"check\", response);\n          this.status = response.data.status;\n          console.log(\"status\", this.status);\n          if (this.status) {\n            console.log(\"here\");\n            return true;\n          } else {\n            return false;\n          }\n        });\n    }\n  }\n};\n</script>\n\n"],"sourceRoot":"src/views"}]}