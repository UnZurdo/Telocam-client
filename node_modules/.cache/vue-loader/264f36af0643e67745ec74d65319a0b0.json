iner>\n\n    <v-container>\n      <div>\n        <v-alert v-model=\"alert\" dismissible type=\"success\">Empresa añadida!</v-alert>\n      </div>\n      <v-layout row justify-start mb-2>\n        <v-btn small flat color=\"rgb(255, 0, 0, 0.2)\" slot=\"activator\">\n          <PopupPractica @projectAdded=\"alert = true\"/>\n        </v-btn>\n        <v-tooltip top>\n          <v-btn small flat color=\"grey\" @click=\"sortBy('practica')\" slot=\"activator\">\n            <v-icon small left>folder</v-icon>\n            <span class=\"caption text-lowercase\">Por nombre</span>\n          </v-btn>\n          <span>Ordenar por título</span>\n        </v-tooltip>\n        <v-tooltip top>\n          <v-btn small flat color=\"grey\" @click=\"sortBy('empresa')\" slot=\"activator\">\n            <v-icon small left>person</v-icon>\n            <span class=\"caption text-lowercase\">Por empresa</span>\n          </v-btn>\n          <span>Ordenar por empresa</span>\n        </v-tooltip>\n      </v-layout>\n      <v-container fluid grid-list-md>\n        <v-layout row wrap>\n          <v-flex d-flex xs12 sm6 md4 flat v-for=\"project in projects\" :key=\"project.key\">\n            <v-card router :to=\"`/admin/practicas/${project.practicas_id}`\" class=\"ma-1\">\n              <v-layout row wrap :class=\"`pa-4 project ${project.status}`\">\n                <v-flex xs12 sm12 md12>\n                  <h3>{{ project.practica }}</h3>\n                </v-flex>\n                <v-flex xs6 sm5 md3 class=\"mr-3\">\n                  <div class=\"caption grey--text\">Empresa</div>\n                  <div>{{project.empresa}}</div>\n                </v-flex>\n                <v-flex xs10 sm10 md8>\n                  <div class=\"caption grey--text\">Studies</div>\n                  <div class=\"caption darken-2 blue-grey--text\">{{ project.studies }}</div>\n                </v-flex>\n                <v-flex xs12 sm12 md12>\n                  <v-divider></v-divider>\n                </v-flex>\n\n                <v-flex xs12 sm12 md12>\n                  <p class=\"caption darken-2 blue-grey--text\">{{ project.Description_P }}</p>\n                </v-flex>\n              </v-layout>\n            </v-card>\n          </v-flex>\n        </v-layout>\n      </v-container>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport PopupPractica from \"../../components/PopupPractica\";\n\nimport axios from \"axios\";\nimport store from \"../../store.js\";\nimport { API_BASE } from \"../../config\";\n\nexport default {\n  data() {\n    return {\n      alert: false,\n      first: true,\n      projects: null,\n      empresa: null,\n      empresas: null,\n      name_company: \"\",\n      ProjectsAndEmpresas: {},\n      id: \"\",\n      token: store.getters.token,\n      snackbar: false\n    };\n  },\n  components: { PopupPractica },\n\n  methods: {\n    sortBy(prop) {\n      if (this.direction) {\n        this.direction = false;\n        this.projects.sort((a, b) => (a[prop] < b[prop] ? -1 : 1));\n      } else {\n        this.direction = true;\n        this.projects.sort((a, b) => (a[prop] > b[prop] ? -1 : 1));\n      }\n    },\n\n    getEmpresa(id) {\n      console.log(\"EmpresaID: \" + id);\n      axios\n        .get(`${API_BASE}/practicas&empresas/all` + id, {\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: \"Bearer \" + localStorage.token\n          }\n        })\n        .then(response => {\n          let empresa = response.data.data;\n          console.log(empresa);\n          return empresa;\n        });\n    }\n  },\n  mounted() {\n    axios\n      .get(`${API_BASE}/practicas&empresas/all`, {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: \"Bearer \" + localStorage.token\n        }\n      })\n      .then(response => {\n        this.projects = response.data.data;\n        console.log(\"Response2\", this.projects);\n\n        //this.empresas = response.data.empresas;\n      });\n  },\n  computed: {\n    // a computed getter\n    empresaName: function() {\n      axios\n        .get(`${API_BASE}/practicas&empresas/all`, {\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: \"Bearer \" + localStorage.token\n          }\n        })\n        .then(response => {\n          this.ProjectsAndEmpresas = response.data.data.forEach((key, val) => {\n            console.log(key, val);\n          });\n          //this.empresas = response.data.empresas;\n        });\n    }\n  }\n};\n</script>\n\n<style>\n.project.complete {\n  border-left: 4px solid #3cd1c2;\n}\n.project.ongoing {\n  border-left: 4px solid #ffaa2c;\n}\n.project.overdue {\n  border-left: 4px solid #f83e70;\n}\n.v-chip.complete {\n  background: #3cd1c2;\n}\n.v-chip.ongoing {\n  background: #ffaa2c;\n}\n.v-chip.overdue {\n  background: #f83e70;\n}\n</style>"]}]}